page{
  width:100%;
  height: 100%;
}
.back{
  position: fixed;
  width: 100%;
  height: 100%;
  -webkit-filter:blur(3px);
  z-index: -1;
}
.RegLogo{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding-top: 100rpx;
  text-align: center;
  color: #FFF;
  font-size: 100rpx;
  font-weight: 700;
}
input{
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 15px;
  margin: 60rpx auto;
  text-align: center;
  border: 1px solid #FFF;
  border-radius: 20px;
  color: #FFF;
}
button{
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 60rpx auto;
  text-align: center;
  border-radius: 20px;
  background-color: #6089b9;
  opacity: 0.8;
  font-size: 20px;
  font-weight: 700;
  color: #FFF;
}
.ts{
  font-size: 13px;
  width: 100%;
  text-align: center;
  margin-top: 40rpx;
  color: #FFF;
}